{
  "name": "cryptoportfolio",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-preset-react-native-stage-0": "^1.0.1",
    "debug": "~3.1.0",
    "jest": "^23.4.2",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.3.1",
    "sinon": "^6.1.4"
  },
  "scripts": {
    "images": "echo \"var images = {};\" > ./images.js; find ./images/*.png -not -name '*@*' -type f -maxdepth 1 | sed -e 's,^\\./images/,,' | sed -e 's,\\.png,,' | while read f; do \n IMGKEY=`echo $f | tr 'a-z' 'A-Z'` \n echo \"images['$IMGKEY'] = require('./images/$f.png');\"; done >> ./images.js; echo \"module.exports = images;\" >> ./images.js",
    "start": "react-native start",
    "start-debug": "DEBUG=app npm start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "<root_folder>/node_modules/(?!react-native|victory-native|victory-animation)"
    ]
  },
  "dependencies": {
    "@icon/cryptocurrency-icons": "0.0.1-4",
    "axios": "^0.18.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "coinmarketcap-icons": "^1.0.0",
    "cryptocurrencies": "git+https://github.com/crypti/cryptocurrencies.git",
    "lodash": "^4.17.10",
    "mobx-react": "^5.2.3",
    "moment": "^2.22.2",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-svg": "^6.5.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "string-format": "^2.0.0",
    "victory-native": "^30.1.0"
  }
}
