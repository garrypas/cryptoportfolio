{
  "name": "cryptoportfolio",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "debug": "~3.1.0",
    "jest-expo": "~27.0.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "sinon": "^6.1.4"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "images": "echo \"var images = {};\" > ./images.js; find ./images/*.png -type f -maxdepth 1 | sed -e 's,^\\./images/,,' | sed -e 's,\\.png,,' | while read f; do \n IMGKEY=`echo $f | tr 'a-z' 'A-Z'` \n echo \"images['$IMGKEY'] = require('./images/$f.png');\"; done >> ./images.js; echo \"module.exports = images;\" >> ./images.js",
    "start": "npm run images && react-native-scripts start",
    "start-debug": "DEBUG=app npm start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "<root_folder>/node_modules/(?!react-native|victory-native|victory-animation)"
    ]
  },
  "dependencies": {
    "@icon/cryptocurrency-icons": "0.0.1-4",
    "coinmarketcap-icons": "^1.0.0",
    "cryptocurrencies": "git+https://github.com/crypti/cryptocurrencies.git",
    "expo": "^27.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-svg": "^6.5.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "victory-native": "^30.1.0"
  }
}
